{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "copypatch@kiszka.org",
      "strict_min_version": "68.0"
    }
  },
  "name": "Copy Patch",
  "description": "Copy email content to clipboard for application as patch",
  "author": "Jan Kiszka",
  "version": "1.3.2",
  "homepage_url": "http://git.kiszka.org/copypatch.git",
  "icons": {
    "32": "copypatch.png",
    "64": "copypatch64.png"
  },
  "legacy": {
    "type" : "bootstrap"
  },
  "experiment_apis": {
    "CopyPatch": {
      "schema": "api/CopyPatch/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "CopyPatch"
          ]
        ],
        "script": "api/CopyPatch/implementation.js"
      }
    }
  }
}
