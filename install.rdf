<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>copypatch@kiszka.org</em:id>
    <em:name>Copy Patch</em:name>
    <em:description>Copy email content to clipboard for application as patch</em:description>
    <em:version>0.3</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:creator>Jan Kiszka</em:creator>
    <em:homepageURL>http://git.kiszka.org/copypatch.git</em:homepageURL>

    <em:iconURL>chrome://copypatch/content/copypatch.png</em:iconURL>
    <em:icon64URL>chrome://copypatch/content/copypatch64.png</em:icon64URL>

    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>60.0</em:minVersion>
        <em:maxVersion>60.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
